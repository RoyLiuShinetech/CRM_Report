      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="CRM_Filter">
            <Value>=Parameters!CRM_Filter.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>&lt;fetch version="1.0" output-format="xml-platform" mapping="logical" distinct="false"&gt;
  &lt;entity name="salesorder"  enableprefiltering="1" prefilterparametername="CRM_Filter"&gt;
    &lt;attribute name="ordernumber" /&gt;  
    &lt;attribute name="new_bestellnummer" /&gt;  
    &lt;attribute name="new_lieferantennummer" /&gt; 
    &lt;attribute name="new_referenz" /&gt; 
    &lt;attribute name="createdon" /&gt;  
    &lt;attribute name="createdby" /&gt;           
       
    &lt;attribute name="requestdeliveryby" /&gt; 
    &lt;attribute name="shippingmethodcode" /&gt;   
    &lt;attribute name="paymenttermscode" /&gt; 
    &lt;attribute name="freighttermscode" /&gt;        

    &lt;attribute name="salesorderid" /&gt;       
	&lt;filter type="and"&gt;
      &lt;condition attribute="name" operator="eq" value="Test_Order1" /&gt;
    &lt;/filter&gt;

    &lt;link-entity name="account" from="accountid" to="customerid" alias="acc"&gt;          		
	  &lt;attribute name="accountnumber" /&gt;  
	  &lt;attribute name="name" /&gt;  
	  
	  &lt;attribute name="address1_name" /&gt;  	
	  &lt;attribute name="address1_line1" /&gt;  
	  &lt;attribute name="address1_line2" /&gt;  
	  &lt;attribute name="address1_postalcode" /&gt;  
	  &lt;attribute name="address1_city" /&gt;  	   
	  &lt;attribute name="address1_country" /&gt;    
    &lt;/link-entity&gt;    
	&lt;link-entity name="account" link-type="outer" to="customerid" alias="acd"&gt;          		
	  &lt;attribute name="accountnumber" /&gt;  
	  &lt;attribute name="name" /&gt;  
	  
	  &lt;attribute name="address1_name" /&gt;  	
	  &lt;attribute name="address1_line1" /&gt;  
	  &lt;attribute name="address1_line2" /&gt;  
	  &lt;attribute name="address1_postalcode" /&gt;  
	  &lt;attribute name="address1_city" /&gt;  	   
	  &lt;attribute name="address1_country" /&gt;  
	  &lt;filter type="and"&gt;
		&lt;condition attribute="name" operator="eq" value="Bucher Transport AG" /&gt;
	  &lt;/filter&gt; 
    &lt;/link-entity&gt;  
  &lt;/entity&gt;
&lt;/fetch&gt;</CommandText>
      </Query>
